<div class="rightSide">
    <div class="navFloor">
        <div class="floor"><a href="/4">P4</a></div>
        <div class="floor"><a href="/3">P3</a></div>
        <div class="floor"><a href="/2">P2</a></div>
        <div class="floor"><a href="/1">P1</a></div>
    </div>
    <div class="container">
        <div class="aa">
            <div class="section">
                <div class="photo">
                    <img src="https://images.unsplash.com/photo-1610186594416-2c7c0131e35d?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=500&q=80"
                        alt="To twoj login: {{login.value}} ?">
                </div>
                <div class="currentFloor" data-floor="{{paramsFloor}}">P{{paramsFloor}}</div>
            </div>
            <div class="login" data-rowname="{{login.name}}">
                Login: <input type="text" class="value" data-rowvalue="{{login.value}}" value="{{login.value}}"></input>
            </div>
        </div>

        <div class="lastUpdate" data-date="{{lastUpdate.value}}">Last Update: {{lastUpdate.value}}</div>
        <div class="keyboard">
            <button data-value="1">1</button>
            <button data-value="2">2</button>
            <button data-value="3">3</button>
            <button data-value="4">4</button>
            <button data-value="5">5</button>
            <button data-value="6">6</button>
            <button data-value="7">7</button>
            <button data-value="8">8</button>
            <button data-value="9">9</button>
            <button data-value="0">0</button>
            <button data-value="clear">Clear</button>
        </div>
        <div class="footer">
            <button class="update">Update</button>
        </div>
    </div>
</div>